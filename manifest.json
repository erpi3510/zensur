{
    "manifest_version": 3,
    "name": "URL-Überprüfungs-Erweiterung",
    "version": "1.0",
    "permissions": [
      "tabs",
      "activeTab",
      "storage",
      "scripting",
      "notifications"
    ],
    "host_permissions": [
      "https://extension.action/*",
      "https://extension.browser_action/*"
  ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      }
    },
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  